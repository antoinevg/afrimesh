
config 'wifi-device' 'wifi0'
	option 'type' 'atheros'
	option 'channel' '1'

config 'wifi-iface'
	option 'device' 'wifi0'
	option 'encryption' 'none'
	option 'ssid'       'potato'
	option 'mode'       'ahdemo'
	option 'bssid'      '01:CA:FF:EE:BA:BE'
	option  swmerge     1
	option  bgscan      0
	option 'network'    'wifi0'

